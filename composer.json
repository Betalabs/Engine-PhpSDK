{
  "name": "betalabs/engine-phpsdk",
  "description": "SDK for Engine",
  "authors": [],
  "require": {
    "php": "^7.1",
    "php-http/guzzle6-adapter": "^1.0",
    "php-di/php-di": "^5.4",
    "nesbot/carbon": "^1.22",
    "aura/router": "^3.1",
    "zendframework/zend-diactoros": "^1.6",
    "symfony/http-kernel": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Betalabs\\Engine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Betalabs\\Engine\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
