{
  "name": "betalabs/engine-phpsdk",
  "description": "SDK for Engine",
  "authors": [],
  "require": {
    "php": "^7.1",
    "php-http/guzzle6-adapter": "^1.0",
    "php-di/php-di": "^5.4|^6.0",
    "nesbot/carbon": "^2.26",
    "aura/router": "^3.1",
    "zendframework/zend-diactoros": "^1.6|^2.0",
    "symfony/cache": "^4.3",
    "symfony/http-kernel": "^4.3",
    "symfony/mime": "^4.3",
    "symfony/http-foundation": "^4.3",
    "symfony/var-dumper": "^4.3",
    "symfony/var-exporter": "^4.3",
    "predis/predis": "^1.1",
    "phpdocumentor/reflection-docblock": "^4.3",
    "phpdocumentor/type-resolver": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Betalabs\\Engine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Betalabs\\Engine\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
